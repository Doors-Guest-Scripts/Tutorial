---====== Load spawner ======---

local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/V2/Source.lua"))()

---====== Create entity ======---

local entity = spawner.Create({
	Entity = {
		Name = "I-666",
		Asset = "rbxassetid://104824938701121",
		HeightOffset = 0
	},
	Lights = {
		Flicker = {
			Enabled = false,
			Duration = 7
		},
		Shatter = false,
		Repair = false
	},
	Earthquake = {
		Enabled = false
	},
	CameraShake = {
		Enabled = true,
		Range = 200,
		Values = {10,10, 5, 5} -- Magnitude, Roughness, FadeIn, FadeOut
	},
	Movement = {
		Speed = 6666,
		Delay = 15,
		Reversed = false
	},
	Rebounding = {
		Enabled = true,
		Type = "Ambush", -- "Blitz"
		Min = 100,
		Max = 100,
		Delay = 0
	},
	Damage = {
		Enabled = true,
		Range = 40,
		Amount = 0.01
	},
	Crucifixion = {
		Enabled = true,
		Range = 40,
		Resist = false,
		Break = true
	},
	Death = {
		Type = "Guiding", -- "Curious"
		Hints = {"You dead to who called I-666", "He is too fast even you cant see him", "When you see him hide quickly as soon as possible", "You did good keep going!"},
		Cause = ""
	}
})

---====== Debug entity ======---

entity:SetCallback("OnSpawned", function()
    print("Entity has spawned")
function GitAud(soundgit,filename)
    SoundName=tostring(SoundName)
    local url=soundgit
    local FileName = filename
    writefile(FileName..".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName..".mp3")
end

function CustomGitSound(soundlink, vol, filename)
    local sound = Instance.new("Sound")
    sound.SoundId = GitAud(soundlink, filename)
    sound.Parent = workspace
    sound.Name = "Theme666"
    sound.Volume = 3
    sound.PlaybackSpeed = 1
   sound:Play()
end

CustomGitSound("https://github.com/vuivuiviu2/Guestly-ngoan-xinh-iu/blob/main/G-95.mp3?raw=true", 1, " I666theme")

game.Lighting.MainColorCorrection.TintColor = Color3.fromRGB(255, 255, 255)
game.Lighting.MainColorCorrection.Contrast = 0
local tween = game:GetService("TweenService")
tween:Create(game.Lighting.MainColorCorrection, TweenInfo.new(2.5), {Contrast = 0}):Play()
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(10),{TintColor = Color3.fromRGB(255, 0, 0)})
TW:Play()
end)

entity:SetCallback("OnStartMoving", function()
    print("Entity has started moving")
end)

entity:SetCallback("OnEnterRoom", function(room, firstTime)
    if firstTime == true then
        print("Entity has entered room: ".. room.Name.. " for the first time")
    else
        print("Entity has entered room: ".. room.Name.. " again")
    end
end)

entity:SetCallback("OnLookAt", function(lineOfSight)
	if lineOfSight == true then
		print("Player is looking at entity")
	else
		print("Player view is obstructed by something")
	end
end)

entity:SetCallback("OnRebounding", function(startOfRebound)
    if startOfRebound == true then
        print("Entity has started rebounding")
	else
        print("Entity has finished rebounding")
	end
end)

entity:SetCallback("OnDespawning", function()
    print("Entity is despawning")
end)

entity:SetCallback("OnDespawned", function()
    print("Entity has despawned")
game.Players.LocalPlayer.PlayerGui.I666DIE:Destroy()
game.Workspace.Theme666:Destroy()
game.Lighting.MainColorCorrection.TintColor = Color3.fromRGB(255, 0, 0)
game.Lighting.MainColorCorrection.Contrast = 0
local tween = game:GetService("TweenService")
tween:Create(game.Lighting.MainColorCorrection, TweenInfo.new(2.5), {Contrast = 0}):Play()
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(5),{TintColor = Color3.fromRGB(255, 255, 255)})
TW:Play()

local achievementGiver = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Custom%20Achievements/Source.lua"))()

---====== Display achievement ======---
achievementGiver({
    Title = "The Ultimate Horror",
    Desc = "Let me cook.",
    Reason = "Encounter I-666.",
    Image = "rbxassetid://77714309161331"
})
end)

entity:SetCallback("OnDamagePlayer", function(newHealth)
	if newHealth == 0 then
		print("Entity has killed the player")
	else
		print("Entity has damaged the player")
local C = game.Players.LocalPlayer.Character:WaitForChild("Collision")
C.Anchored = true
wait(5)
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(3),{Contrast = 100})
TW:Play()

local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(3),{Saturation = 100})
TW:Play()
wait(2)
game.Players.LocalPlayer.Character.Humanoid.Health -= 100

game.ReplicatedStorage.GameStats["Player_".. game.Players.LocalPlayer.Name].Total.DeathCause.Value = "I-666"

wait(1)
game.Lighting.MainColorCorrection.Contrast = 0
game.Lighting.MainColorCorrection.Saturation = 0
	end
end)

--[[

DEVELOPER NOTE:
By overwriting 'CrucifixionOverwrite' the default crucifixion callback will be replaced with your custom callback.

entity:SetCallback("CrucifixionOverwrite", function()
    print("Custom crucifixion callback")
end)

]]--

---====== Run entity ======---

entity:Run()

wait(15)
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "I666DIE"
screenGui.Parent = playerGui
screenGui.IgnoreGuiInset = true

local red1 = Instance.new("ImageLabel")
red1.Name = "DIE"
red1.Size = UDim2.new(1, 0, 1, 0)
red1.BackgroundTransparency = 1 -- Make the background fully transparent
red1.ImageTransparency = 0
red1.Parent = screenGui

task.spawn(function()
while true do
red1.Image = "rbxassetid://98465300620838"
wait(1.5)
red1.Image = "rbxassetid://71820011049841"
wait(1.5)
red1.Image = "rbxassetid://121691039738175"
wait(1.5)
red1.Image = "rbxassetid://99216723644435"
wait(1.5)
red1.Image = "rbxassetid://95942925787503"
wait(1.5)
red1.Image = "rbxassetid://90310097411511"
wait(1.5)
end
end)
