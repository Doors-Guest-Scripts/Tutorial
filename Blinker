_G.A90Config = {
    Face = "rbxassetid://82754831225080",
    AngryFace = "rbxassetid://138686166387390",
    StopIcon = "rbxassetid://135322410847454",
    StaticColor = Color3.fromRGB(78, 3, 255),
    HitSound = "rbxassetid://94146776840470",
    SpawnSound = "rbxassetid://632831227",
    Damage = 25
}

-- Made by @Focuslol666
function RunA90()
    local config = _G.A90Config

    local gui = game:GetService("Players").LocalPlayer.PlayerGui
    gui.MainUI.Jumpscare.Jumpscare_A90.Face.Image = config.Face
    gui.MainUI.Jumpscare.Jumpscare_A90.FaceAngry.Image = config.AngryFace
    gui.MainUI.Jumpscare.Jumpscare_A90.StopIcon.Image = config.StopIcon
    gui.MainUI.Jumpscare.Jumpscare_A90.Static.ImageColor3 = config.StaticColor
    gui.MainUI.Jumpscare.Jumpscare_A90.Static2.ImageColor3 = config.StaticColor
    gui.MainUI.Jumpscare.Jumpscare_A90.StopIcon.StopStatic.ImageColor3 = config.StaticColor
    gui.MainUI.Initiator.Main_Game.RemoteListener.Modules.A90.Hit.SoundId = config.HitSound
    gui.MainUI.Initiator.Main_Game.RemoteListener.Modules.A90.Spawn.SoundId = config.SpawnSound

    coroutine.wrap(function()
        local initiator = gui.MainUI.Initiator.Main_Game
        require(initiator.RemoteListener.Modules.A90)(
            require(initiator),
            workspace.CurrentRooms[game.Players.LocalPlayer:GetAttribute("CurrentRoom")]
        )

        while true do
            if gui.MainUI.Jumpscare.Jumpscare_A90.FaceAngry.Visible then
                task.wait(1)
                game.Players.LocalPlayer.Character.Humanoid.Health -= config.Damage
                break
            end
            task.wait()
        end
        if game:GetService("Players").LocalPlayer.Character.Humanoid.Health <= 0 then
            game:GetService("ReplicatedStorage").GameStats["Player_".. game.Players.LocalPlayer.Name].Total.DeathCause.Value = "Blinker"
            firesignal(game.ReplicatedStorage.RemotesFolder.DeathHint.OnClientEvent, {"You died to Blinker", "Dont move when he appears"},"Blue")
        end
    end)()
end

RunA90()

local cue = Instance.new("Sound")
	cue.Parent = game.Workspace
	cue.Name = "Spawn"
	cue.SoundId = "rbxassetid://1201103772"
	cue.Volume = 1
	cue.PlaybackSpeed = 1.5
	cue:Play()
game.Lighting.MainColorCorrection.TintColor = Color3.fromRGB(255, 107, 8)
game.Lighting.MainColorCorrection.Contrast = 10
local tween = game:GetService("TweenService")
tween:Create(game.Lighting.MainColorCorrection, TweenInfo.new(2.5), {Contrast = 0}):Play()
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(5),{TintColor = Color3.fromRGB(255, 255, 255)})
TW:Play()
local caption = game.Players.LocalPlayer:WaitForChild("PlayerGui").MainUI.MainFrame.Caption
caption.TextColor3 = Color3.fromRGB(3, 255, 11)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Toxicity: +1%",true)
task.wait(0)
_G.stats.Value += 1

---====== Load achievement giver ======---
local achievementGiver = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Custom%20Achievements/Source.lua"))()

---====== Display achievement ======---
achievementGiver({
    Title = "Deadly Obedience",
    Desc = "Move please..",
    Reason = "Encounter Blinker.",
    Image = "rbxassetid://101667449789753"
})
