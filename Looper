---====== Display achievement ======---
achievementGiver({
    Title = "Repeat Many Times",
    Desc = "Why are you repeating ur ways?",
    Reason = "Encounter Looper",
    Image = "rbxassetid://132295172147649"
})
end)

entity:SetCallback("OnDamagePlayer", function(newHealth)
	if newHealth == 0 then
		print("Entity has killed the player")
