    sound.Volume = 3
   sound:Play()
end

CustomGitSound("https://github.com/DoorsHubs/Tutorial/raw/refs/heads/main/lv_0_20250602152425.mp3?raw=true", 1, " StartTheme")

game.Lighting.MainColorCorrection.TintColor = Color3.fromRGB(3, 255, 28)
game.Lighting.MainColorCorrection.Contrast = 10
local tween = game:GetService("TweenService")
tween:Create(game.Lighting.MainColorCorrection, TweenInfo.new(2.5), {Contrast = 0}):Play()
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(5),{TintColor = Color3.fromRGB(255, 255, 255)})
TW:Play()

local Kill = true
local eye = game:GetObjects("rbxassetid://86935728207882")[1]
eye.Parent = game.Workspace
local eyes = eye:FindFirstChildWhichIsA("BasePart") or eye:FindFirstChildWhichIsA("Part")
eyes.CFrame = game.Workspace.CurrentRooms:GetChildren()[#game.Workspace.CurrentRooms:GetChildren() - 1].Door.Door.CFrame + Vector3.new(10,0,20)

wait(3)
task.spawn(function()
    while Kill == true do
        task.wait(0.3)
        local _ , camera = game.Workspace.CurrentCamera:WorldToViewportPoint(eyes.Position)
        if camera then
                game.Players.LocalPlayer.Character.Humanoid:TakeDamage(5)
        end
    if game.Players.LocalPlayer.Character.Humanoid.Health <= 0 then
                game.ReplicatedStorage.GameStats["Player_".. game.Players.LocalPlayer.Name].Total.DeathCause.Value = "Coercion's Clone"
    end
   end
end)

wait(5)
local caption = game.Players.LocalPlayer:WaitForChild("PlayerGui").MainUI.MainFrame.Caption
caption.TextColor3 = Color3.fromRGB(3, 252, 3)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("*Spawn Clone*",true)

